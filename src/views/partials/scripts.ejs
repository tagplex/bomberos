<script type="text/javascript">
  function formatCliente(run) {
    run.value = run.value.replace(/[.-]/g, '')
      .replace(/^(\d{1,2})(\d{3})(\d{3})(\w{1})$/, '$1.$2.$3-$4')
  }
</script>